}
.goThereContainer{
  width: 80px;
  height: 80px;
  display: flex;
  margin-top: 300px;
  color: white;
  border-radius: 50%;
  flex-direction: column;
  justify-content: space-between;
}
.goThereContainer image{
  width: 60px;
  height: 60px;
  background-color: white;
  border-radius: 50%;
  align-items: center;
}
.goThereContainer span{
  font-weight: 600;
  text-align: center;
}