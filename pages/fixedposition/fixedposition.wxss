.navbarContainer {
  height: 100%;
  display: flex;
  flex-direction: column;
}
.navbar {
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 500;
  width: 100%;
  height: 35px;
  flex-direction: row;
  text-align: center;
  color: #a8a8a8;
  font-size: 12px;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 1rpx solid #dfdfdf;
}
.navbarItem {
  flex: 1;
  padding: 20rpx 0px;
}
.navbarItemActive {
  font-weight: 800;
  color: #f85a1c;
}
/* 下边框左右动画 */
.line {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 4rpx;
  background: red;
  transition: all 0.3s ease-out 0.1s;
}
.navbarSwiper {
  padding-top: 50px;
}
